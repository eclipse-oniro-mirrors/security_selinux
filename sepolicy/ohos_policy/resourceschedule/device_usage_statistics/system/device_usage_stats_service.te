type device_usage_stats_service, sadomain, domain;

allow device_usage_stats_service accesstoken_service:binder { call };
allow device_usage_stats_service accountmgr:binder { call };
allow device_usage_stats_service data_file:dir { search };
allow device_usage_stats_service data_service_el1_file:dir { open read search };
allow device_usage_stats_service data_service_el1_file:file { getattr ioctl lock open read write };
allow device_usage_stats_service data_service_file:dir { search };
allow device_usage_stats_service dev_unix_socket:dir { search };
allow device_usage_stats_service foundation:binder { call };
allow device_usage_stats_service time_service:binder { call };
allowxperm device_usage_stats_service data_service_el1_file:file ioctl 0xf50c;

#avc:  denied  { get } for service=3301 pid=282 scontext=u:r:device_usage_stats_service:s0 tcontext=u:object_r:sa_foundation_powermgr_service:s0 tclass=samgr_class permissive=1
allow device_usage_stats_service sa_foundation_powermgr_service:samgr_class { get };
